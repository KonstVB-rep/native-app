{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "types": [
      "react",
      "react-native"
    ],
    "lib": [
      "DOM",
      "ESNext"
    ],
    "jsx": "react-native",
    "moduleSuffixes": [
      ".ios",
      ".native",
      "andoid",
      ""
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}